{
  "$schema": "https://frontmatter.codes/frontmatter.schema.json",
  "frontMatter.framework.id": "hugo",
  "frontMatter.content.publicFolder": "static",
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "default",
      "previewPath": null,
      "pageBundle": false,
      "fields": [
        {
          "title": "Title",
          "name": "title",
          "type": "string"
        },
        {
          "title": "Description",
          "name": "description",
          "type": "string"
        },
        {
          "title": "Publishing date",
          "name": "date",
          "type": "datetime"
        },
        {
          "title": "Article preview",
          "name": "image",
          "isPreviewImage": true,
          "type": "image"
        },
        {
          "title": "Is in draft",
          "name": "draft",
          "type": "draft"
        },
        {
          "title": "Tags",
          "name": "tags",
          "type": "tags"
        },
        {
          "title": "Categories",
          "name": "categories",
          "type": "categories"
        },
        {
          "title": "authors",
          "name": "authors",
          "type": "choice",
          "choices": [
            "Lefebvre Saboya"
          ]
        },
        {
          "title": "keywords",
          "name": "keywords",
          "type": "choice",
          "choices": [
            "Dale Carnegie",
            "Soft skills",
            "Hard skills",
            "Auto-ajuda",
            "Warren Buffett",
            "Desenvolvimento Pessoal",
            "Habilidades Interpessoais"
          ]
        },
        {
          "title": "aliases",
          "name": "aliases",
          "type": "choice",
          "choices": [
            "/dale-carnegie-vale-a-pena/",
            "/post/dale-carnegie-vale-a-pena/",
            "/blog/dale-carnegie-vale-pena/",
            "/p/dale-carnegie-vale-pena/"
          ]
        }
      ]
    }
  ],
  "frontMatter.content.pageFolders": [
    {
      "title": "images",
      "path": "[[workspace]]/assets/images"
    },
    {
      "title": "content",
      "path": "[[workspace]]/content"
    },
    {
      "title": "static",
      "path": "[[workspace]]/static"
    },
    {
      "title": "blog",
      "path": "[[workspace]]/content/pt-br/blog"
    }
  ],
  "frontMatter.content.autoUpdateDate": true,
  "frontMatter.preview.host": "http://localhost:1313"
}