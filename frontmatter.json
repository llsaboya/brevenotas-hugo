{
  "$schema": "https://frontmatter.codes/frontmatter.schema.json",
  "frontMatter.framework.id": "hugo",
  "frontMatter.content.publicFolder": "assets",
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "default",
      "previewPath": null,
      "pageBundle": false,
      "fields": [
        {
          "title": "Title",
          "name": "title",
          "type": "string"
        },
        {
          "title": "Description",
          "name": "description",
          "type": "string"
        },
        {
          "title": "Publishing date",
          "name": "date",
          "type": "datetime"
        },
        {
          "title": "Article preview",
          "name": "image",
          "isPreviewImage": true,
          "type": "image"
        },
        {
          "title": "Is in draft",
          "name": "draft",
          "type": "draft"
        },
        {
          "title": "Tags",
          "name": "tags",
          "type": "tags"
        },
        {
          "title": "Categories",
          "name": "categories",
          "type": "categories"
        },
        {
          "title": "authors",
          "name": "authors",
          "type": "choice",
          "choices": [
            "Lefebvre Saboya"
          ]
        },
        {
          "title": "keywords",
          "name": "keywords",
          "type": "choice",
          "choices": [
            "Tag",
            "Tags"
          ]
        }
      ]
    }
  ],
  "frontMatter.taxonomy.categories": [
    "Comunicação",
    "Cultura",
    "Instructional Design",
    "Negócios",
    "Notas",
    "Política",
    "Tecnologia"
  ],
  "frontMatter.content.pageFolders": [
    {
      "title": "images",
      "path": "[[workspace]]/static"
    },
    {
      "title": "content",
      "path": "[[workspace]]/content"
    }
  ],
  "frontMatter.dashboard.openOnStart": true,
  "frontMatter.framework.startCommand": "hugo server",
  "frontMatter.templates.enabled": true
}